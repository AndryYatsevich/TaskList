.modal {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 9000;
    width: 100%;
    height: 100%;
    background-color: rgba(91,91,91,0.7);
}

.modal-body {
    z-index: 9001;
    margin: 200px auto;
    width: 50%;
    border: 2px solid black;
    border-radius: 15px
    background-color: #048B72;
    //background: url('./img/modalBackground.jpg') no-repeat cover;

}